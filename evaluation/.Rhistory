create_new_crps_skill <- function(d, title = "Offshore Wind Power", lab = TRUE,
ylimits = c(-5, 25)) {
require(data.table)
d[["Name"]] <- as.factor(row.names(d))
d[["Model"]] <- as.factor(c(rep("Linear Regression", 3), rep("Random Forest", 3)))
d[["Weather"]] <- as.factor(c(rep(c("One-Step P", "One-Step W", "Two-Step WP"), 2)))
long <- melt(d, id.vars = c("Name", "Model", "Weather"),
variable.name = "Horizon")
long$Horizon <- as.numeric(as.character((long$Horizon)))
if (lab == TRUE) {
g.skill <- ggplot(long, aes(y = value, x = Horizon, col = Weather, group = Name)) +
geom_line(size = 1) +
geom_hline(yintercept=0, linetype="dashed", color = "black", size=1) +
geom_point(aes(shape = Model), size = 4) +
ylab('CRPS Skill') +
scale_color_viridis_d(option = "magma", begin = 0.2, end = 0.8) +
ylim(ylimits) +
scale_x_continuous(breaks = c(3, 6, 9, 12, 15, 18, 21, 24)) +
labs(col = "Ensemble Post-Processing Strategy:",
shape = "Forecasting Model:") +
theme_bw() +
theme(legend.position="bottom",
legend.box="vertical",
legend.margin=margin(),
panel.border = element_blank()) +
ggtitle(title)
} else {
g.skill <- ggplot(long, aes(y = value, x = Horizon, col = Weather, group = Name)) +
geom_line(size = 1) +
geom_hline(yintercept=0, linetype="dashed", color = "black", size=1) +
geom_point(aes(shape = Model), size = 4) +
ylab('CRPS Skill') +
scale_color_viridis_d(option = "magma", begin = 0.2, end = 0.8) +
ylim(ylimits) +
scale_x_continuous(breaks = c(3, 6, 9, 12, 15, 18, 21, 24)) +
theme_bw() +
theme(legend.position="none",
panel.border = element_blank()) +
ggtitle(title)
}
return(g.skill)
}
pdf.plot <- function(p, n = "graph", w = 14/2.54, h = 10/2.54){
grDevices::pdf(file = paste0(n, ".pdf"),
width = w,
height = h)
plot(p)
graphics.off()
}
